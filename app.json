{
  "expo": {
    "name": "test-app-nrs",
    "slug": "test-app-nrs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "testappnrs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fff",
          "resizeMode": "contain",
          "image": "./assets/images/app-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e9466c10-32bb-471d-88b9-0762091d58ec"
      }
    },
    "android": {
      "package": "com.test.app.nrs"
    },
    "ios": {
      "bundleIdentifier": "com.test.app.nrs"
    }
  }
}
